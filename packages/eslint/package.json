{
  "name": "@homestuck/eslint-config",
  "version": "1.6.0",
  "private": false,
  "description": "Baseline ESLint configs used and maintained by Homestuck Inc., et al.",
  "repository": {
    "type": "git",
    "url": "https://github.com/homestuck/configs.git",
    "directory": "packages/eslint"
  },
  "license": "MIT",
  "author": "Homestuck <support@homestuck.com> (https://homestuck.com)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./eslint.config.mjs",
      "import": "./eslint.config.mjs",
      "node": "./eslint.config.mjs",
      "require": "./eslint.config.mjs",
      "default": "./eslint.config.mjs"
    },
    "./next": {
      "types": "./next.mjs",
      "import": "./next.mjs",
      "node": "./next.mjs",
      "require": "./next.mjs",
      "default": "./next.mjs"
    },
    "./react": {
      "types": "./react.mjs",
      "import": "./react.mjs",
      "node": "./react.mjs",
      "require": "./react.mjs",
      "default": "./react.mjs"
    }
  },
  "main": "./eslint.config.mjs",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "inspect:eslint": "eslint --inspect-config --config ./eslint.config.mjs",
    "inspect:next": "eslint --inspect-config --config ./next.mjs",
    "inspect:react": "eslint --inspect-config --config ./react.mjs",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@homestuck/prettier-config",
  "dependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.5",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.5",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^10.2.0",
    "eslint-plugin-turbo": "^2.7.6",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.1.0",
    "typescript-eslint": "^8.54.0"
  },
  "devDependencies": {
    "@homestuck/prettier-config": "workspace:*",
    "@homestuck/tsconfig": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "catalog:",
    "prettier": "catalog:"
  }
}
